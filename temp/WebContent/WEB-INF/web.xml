<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>temp</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
      </welcome-file-list>
      
      <servlet>
      <servlet-name>insert</servlet-name>
      <servlet-class>com.se.temp.DataStoreServlet</servlet-class>  
      </servlet>
      <servlet-mapping>
      <servlet-name>insert</servlet-name>
      <url-pattern>/ds</url-pattern>
      </servlet-mapping>
      
      
      
      <servlet>
      <servlet-name>display</servlet-name>
      <servlet-class>com.se.temp.DisplayServlet</servlet-class>  
      </servlet>
      <servlet-mapping>
      <servlet-name>display</servlet-name>
      <url-pattern>/dis</url-pattern>
      </servlet-mapping>
      
      
      
      <servlet>
      <servlet-name>delete</servlet-name>
      <servlet-class>com.se.temp.DeleteServlet</servlet-class>  
      </servlet>
      <servlet-mapping>
      <servlet-name>delete</servlet-name>
      <url-pattern>/del</url-pattern>
      </servlet-mapping>
      
      
      
      <servlet>
      <servlet-name>update</servlet-name>
      <servlet-class>com.se.temp.UpdateServlet</servlet-class>  
      </servlet>
      <servlet-mapping>
      <servlet-name>update</servlet-name>
      <url-pattern>/us</url-pattern>
      </servlet-mapping>
      
      
      
      <servlet>
      <servlet-name>retrive</servlet-name>
      <servlet-class>com.se.temp.RetriveServlet</servlet-class>  
      </servlet>
      <servlet-mapping>
      <servlet-name>retrive</servlet-name>
      <url-pattern>/rs</url-pattern>
      </servlet-mapping>
      
      
      
</web-app>